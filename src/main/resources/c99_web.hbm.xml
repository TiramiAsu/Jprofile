<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="c99_web.model.Products" table="ICProducts">
        <id column="PD_ID" name="pdID" type="int">
            <generator class="native"/>
        </id>
        <property name="pdCode" type="string">
            <column name="PD_CODE" length="10" not-null="true" />
        </property>
        <property name="pdName" type="string">
            <column name="PD_NAME" length="20" not-null="true" />
        </property>
        <property name="pdPrice" type="java.lang.Integer">
            <column name="PD_PRICE" not-null="true" />
        </property>
        <property name="pdCost" type="java.lang.Integer">
            <column name="PD_COST" not-null="true" />
        </property>
        <property name="pdUnit" type="string">
            <column name="PD_UNIT" length="10" not-null="true" />
        </property>
        <property name="pdStatus" type="string">
            <column name="PD_STATUS" length="10" not-null="true" />
        </property>
        <property name="pdRemark" type="string">
            <column name="PD_REMARK" length="50"/>
        </property>
    </class>
</hibernate-mapping>